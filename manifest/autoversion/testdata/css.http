    <div class=fundraiser>
        <div class=fundraiser-heading>
            <strong>ZSF Needs Money!</strong>
            <a href=/news/2024-financials/#2024-financial-report-and-fundraiser>Announcing Our 2024 Fundraiser</a>
        </div>
        <div class=container>
            <div class=progress>
                <div class=time>
                    <span id=days-remaining>...</span>
                     Days Remaining
                </div>
                <div id=bar-bg>
                    <div id=bar-fg style=width:0%>
                        <span id=amt-raised>loading...</span>
                         per month raised
                    </div>
                </div>
                <div style=font-size:.6em>Goal: Pay 1 contributor for 20 hours/week</div>
                <script>
                function updateSupporters() {
                    let ms_diff = (new Date("Feb 18 2024")) - (new Date());
                    let day_diff = Math.ceil(ms_diff / 1000 / 60 / 60 / 24);
                    document.getElementById("days-remaining").textContent = day_diff;
                    let sponsor_amt = 3802;
                    let sponsor_goal = 5200;
                    let percent = Math.min((sponsor_amt / sponsor_goal), 1)
                    document.getElementById("bar-fg").style.width = (percent * 100) + "%";
                    document.getElementById("amt-raised").textContent = "$" + sponsor_amt + "/" + sponsor_goal;
                }
                setInterval(updateSupporters, 60 * 1000);
                updateSupporters();
                </script>
